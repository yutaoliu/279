\newcommand{\HAT}[1]{\hat{\mathbf{#1}}}
\newcommand{\ARROW}[1]{ \overset{\rightarrow}{\mathbf{#1}} }
\newcommand{\VEC}[3]{\left[\begin{array}{c} #1\\ #2\\#3\\ \end{array} \right]}
\newcommand{\DET}[9]{\begin{vmatrix} #1 & #2 & #3\\ #4 & #5 & #6\\#7 & #8 & #9 \end{vmatrix} }
\newcommand{\MAT}[9]{\left[\begin{array}{ccc} #1 & #2 & #3\\ #4 & #5 & #6\\#7 & #8 & #9 \end{array} \right]}
\newcommand{\FRAME}[1]{ \mathcal{#1} } 

\newcommand{\POS}[2]  { \tensor*[^{#1}]{ \ARROW{r}  }{^{#2}} }
\newcommand{\VEL}[2]  { \tensor*[^{\FRAME{#1}}]{ \ARROW{v} }{^{#2}} }
\newcommand{\ACC}[2]  { \tensor*[^{\FRAME{#1}}]{ \ARROW{a} }{^{#2}} }
\newcommand{\OMEGA}[2]{ \tensor*[^{\FRAME{#1}}]{ \ARROW{\bm{\omega}} }{^{\mathcal{#2} }} }
\newcommand{\ALPHA}[2]{ \tensor*[^{\FRAME{#1}}]{ \ARROW{\bm{\alpha}} }{^{\mathcal{#2} }} }

\newcommand{\DIFF}[2]{ \frac{\tensor*[^{\FRAME{#1} }]{d}{}}{dt}\left[ #2 \right]}
\newcommand{\ROT}[2]{\overset{\HAT{#2}\leftarrow\HAT{#1}}{\mathbf{R}}}



